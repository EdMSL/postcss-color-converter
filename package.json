{
  "name": "postcss-color-converter",
  "version": "1.1.2",
  "description": "Post CSS plugin for color conversion.",
  "keywords": [
    "postcss-plugin",
    "color",
    "converter"
  ],
  "scripts": {
    "test": "mocha ./test/*.test.js",
    "lint": "eslint --config ./eslint-full.config.mjs **/*.js --fix",
    "postcss": "postcss ./test/fixtures/*.test.{css,scss} --config ./test/fixtures/postcss.config.mjs --replace",
    "pack": "npm pack",
    "release": "npm login && npx clean-publish"
  },
  "author": "Eduard Trait <eduard2011rus@gmail.com> (https://github.com/EdMSL)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/EdMSL/postcss-color-converter.git"
  },
  "homepage": "https://github.com/EdMSL/postcss-color-converter#readme",
  "bugs": {
    "url": "https://github.com/EdMSL/postcss-color-converter/issues"
  },
  "dependencies": {
    "color-convert": "^3.1.0",
    "color-name": "^2.0.0",
    "postcss-values-parser": "^6.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "chai": "^5.2.0",
    "clean-publish": "^5.2.2",
    "eslint": "^9.29.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-n": "^17.20.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-security": "^3.0.1",
    "globals": "^16.2.0",
    "mocha": "^11.6.0",
    "postcss": "^8.0.0",
    "postcss-cli": "^11.0.1",
    "postcss-scss": "^4.0.9"
  },
  "peerDependencies": {
    "postcss": "^8.0.0"
  },
  "eslintConfig": {
    "extends": "eslint-config-postcss"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "index.js",
    "src"
  ],
  "type": "module"
}
